## common Makefile rules for building the C - soap samples
##

SOAP_SERVER = $(SOAP_SERVER_SRC) $(SOAP_H_FILES) $(SOAP_NS)
SOAP_CLIENT = $(SOAP_CLIENT_SRC) $(SOAP_H_FILES) $(SOAP_NS)
#$(SOAPHEADER)
$(SOAP_SERVER) :
	$(SOAP) $(SOAP_SERVER_FLAGS) $(SOAP_INFILE)
$(SOAP_CLIENT) :
	$(SOAP) $(SOAP_CLIENT_FLAGS) $(SOAP_INFILE)

LDADD = $(SOAP_C_LIB) $(SAMPLE_EXTRA_LIBS)
BUILT_SOURCES = $(SOAP_SERVER) $(SOAP_CLIENT)



